{
  "name": "game-bug-porter",
  "description": "HTML5 Game 'BugPorter'",
  "version": "0.1.0",
  "scripts": {
    "lint": "./node_modules/.bin/standard | ./node_modules/.bin/snazzy",
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/webpack --progress --profile",
    "build-prod": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/webpack --optimize-minimize",
    "start": "npm run build && ./node_modules/.bin/webpack-dev-server --inline --progress --port 8080"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": ["Phaser"]
  },
  "license": "MIT",
  "author": "Anton Fisher <a.fschr@gmail.com> (http://antonfisher.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonfisher/game-bug-porter.git"
  },
  "bugs": {
    "url": "https://github.com/antonfisher/game-bug-porter/issues"
  },
  "homepage": "https://github.com/antonfisher/game-bug-porter",
  "keywords": [
    "html5",
    "game",
    "webpack"
  ],
  "engines": {
    "node": ">5"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-decorators-legacy": "1.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-stage-0": "6.x",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "0.x",
    "es6-promise": "3.x",
    "es6-shim": "0.x",
    "extract-text-webpack-plugin": "1.x",
    "html-webpack-plugin": "2.x",
    "node-sass": "3.x",
    "postcss-loader": "0.x",
    "rimraf": "2.x",
    "sass-loader": "3.x",
    "snazzy": "4.x",
    "standard": "7.x",
    "style-loader": "0.x",
    "webpack": "1.x",
    "webpack-dev-server": "1.x"
  }
}
